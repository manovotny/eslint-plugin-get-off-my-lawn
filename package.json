{
    "name": "eslint-plugin-get-off-my-lawn",
    "description": "Highly opinionated, ESLint plugin with custom rules to produce beautiful, readable JavaScript.",
    "version": "1.0.2",
    "author": {
        "name": "Michael Novotny",
        "email": "manovotny@gmail.com",
        "url": "https://manovotny.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/manovotny/eslint-plugin-get-off-my-lawn.git"
    },
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin"
    ],
    "engines": {
        "node": ">=10.0.0",
        "yarn": ">=1.0.0"
    },
    "main": "index.js",
    "files": [
        "docs",
        "rules",
        "index.js",
        "!**/__tests__/**"
    ],
    "scripts": {
        "coverage": "jest --coverage",
        "lint": "eslint . --ext .js",
        "prettier": "prettier --write \"**/*.{js,json,md,yml}\"",
        "prettier:check": "prettier --check \"**/*.{js,json,md,yml}\"",
        "test": "yarn lint && yarn prettier:check && yarn coverage",
        "unit": "jest"
    },
    "git": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,json,md,yml}": [
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "dot-prop": "6.0.1",
        "import-modules": "2.1.0",
        "lint-staged": "10.5.4"
    },
    "devDependencies": {
        "@zeit/git-hooks": "0.1.4",
        "chance": "1.1.7",
        "chance-path": "1.0.2",
        "eslint": "7.20.0",
        "jest": "26.6.3",
        "prettier": "2.2.1",
        "prettier-config-get-off-my-lawn": "1.0.0"
    },
    "peerDependencies": {
        "eslint": ">=7.8.1"
    }
}
